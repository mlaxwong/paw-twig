<paw-ckeditor
    {%- if id is defined %} id="{{ id }}"{% endif %}
    {%- if name is defined %} name="{{ name }}"{% endif %}
    {%- if value is defined %} value="{{ value }}"{% endif %}
    {%- if label is defined %} placeholder="{{ label }}"{% endif %}
    {%- if options is defined -%}
    {%- for key, value in options -%}
    {{ key }}="{{ value }}"
    {%- endfor -%}
    {%- endif -%}
></paw-ckeditor>